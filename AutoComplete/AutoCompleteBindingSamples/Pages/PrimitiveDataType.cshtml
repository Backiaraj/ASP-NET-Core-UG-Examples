@page
@model AutoCompleteBindingSamples.Pages.PrimitiveDataType

@{
    var primitiveType = new Records().RecordModelList();
}

<div class="control-wrapper">
    <div id="default" style='padding-top:75px;margin:0 auto;width:250px;'>
        <ejs-autocomplete id="records" dataSource="@primitiveType" placeholder="e.g. Item 1" allowFiltering="true" value="@ViewBag.value" popupheight="200px">
        </ejs-autocomplete>
    </div>
</div>